  "components": {
    "schemas": {
      "Article": {
        "type": "object",
        "required": [
          "title",
          "description",
          "price",
          "stockQuantity",
          "available",
          "brand",
          "searchingKeywords"
        ],
        "properties": {
          "_id": {
            "type": "string",
            "format": "BSON",
            "readOnly": true
          },
          "title": {
            "type": "string",
            "example": "Xiaomi 13T Pro"
          },
          "description": {
            "type": "string",
            "example": "A really amazing Smartphone"
          },
          "price": {
            "type": "number",
            "format": "double",
            "multipleOf": 0.01,
            "example": 790.01
          },
          "stockQuantity": {
            "type": "integer",
            "example": 1000
          },
          "available": {
            "type": "boolean",
            "example": true
          },
          "brand": {
            "type": "string",
            "example": "Xiaomi"
          },
          "searchingKeywords": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["Smartphone", "Xiaomi", "keyword3"]
          },
          "seller": {
            "type": "string",
            "format": "BSON",
            "readOnly": true
          },
          "articlePictures": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uri"
            },
            "example": ["/basic-frontend/src/app/assets/Test.png"]
          }
        }
      },
      "ArticlePatch": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "format": "BSON",
            "readOnly": true
          },
          "title": {
            "type": "string",
            "example": "Xiaomi 13T Pro"
          },
          "description": {
            "type": "string",
            "example": "A really amazing Smartphone"
          },
          "price": {
            "type": "number",
            "format": "double",
            "multipleOf": 0.01,
            "example": 790.01
          },
          "stockQuantity": {
            "type": "integer",
            "example": 1000
          },
          "available": {
            "type": "boolean",
            "example": true
          },
          "brand": {
            "type": "string",
            "example": "Xiaomi"
          },
          "searchingKeywords": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "example": ["Smartphone", "Xiaomi", "keyword3"]
          },
          "seller": {
            "type": "string",
            "format": "BSON",
            "example": "cWGtlAnhcsVViidg",
            "readOnly": true
          },
          "articlePictures": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uri"
            },
            "example": ["/basic-frontend/src/app/assets/Test.png"]
          }
        }
      },
      "Order": {
        "type": "object",
        "required": [
          "buyer",
          "articles",
          "status"
        ],
        "properties": {
